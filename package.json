    "build": "cross-env NODE_ENV=production webpack -p",
    "watch": "webpack --watch",
    "watch:test": "jest --watchAll",
    "prepare": "npm i",
    "clean": "rimraf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/totallytyped/typed-patterns.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/totallytyped/typed-patterns/issues"
  },
  "homepage": "https://github.com/totallytyped/typed-patterns#readme",
  "dependencies": {
    "history": "^2.0",
    "prismjs": "^1.5.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0",
    "tslib": "^1.4.0"
  },
  "devDependencies": {
    "@types/history": "^2.0.41",
    "@types/jest": "^16.0.2",
    "@types/prismjs": "^1.4.18",
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.19",
    "@types/react-router": "^2.0.41",
    "cross-env": "^1.0.7",
    "css-loader": "^0.24.0",
    "jest": "^18.0.0",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^15.3.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/jest-preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.tsx?$"
  }
