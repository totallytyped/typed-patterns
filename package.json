  "dependencies": {
    "history": "^2.0",
    "prismjs": "^1.5.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0"
  },
  "devDependencies": {
    "cross-env": "^1.0.7",
    "css-loader": "^0.24.0",
    "jest-cli": "^15.1.1",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^15.3.1",
    "rimraf": "^2.5.4",
